class Solution {
    public int[] sortArrayByParity(int[] nums) {
         int[] result = new int[nums.length];
        int start = 0;
        int end = nums.length - 1;
        for (int k : nums) {
            if (k % 2 == 0) {
                result[start] = k;
                start++;
            } else {
                result[end] = k;
                end--;
            }
        }
        return result;
    }
}
